{
  "name": "bomberman",
  "version": "1.0.0",
  "repository": "git@github.com:JesusSilva/bomberman.git",
  "license": "MIT",
  "scripts": {
    "dev": "clear && yarn run clear-cache && parcel index.html",
    "clear-cache": "rimraf docs dist .parcel-cache",
    "build": "yarn run clear-cache && yarn run ghbuild",
    "ghbuild": "parcel build --dist-dir docs --public-url /bomberman index.html"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  }
}