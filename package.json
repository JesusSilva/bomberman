{
  "name": "bomberman",
  "version": "1.0.0",
  "description": "Game created with Canvas and Typescript",
  "scripts": {
    "dev": "parcel index.html",
    "start": "yarn run build && parcel ./docs/index.html",
    "build": "yarn run clean && yarn run ghbuild",
    "clean": "rimraf docs dist .parcel-cache",
    "ghbuild": "parcel build --dist-dir docs --public-url ./ index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesussilva/bomberman.git"
  },
  "keywords": [
    "Bomberman",
    "Canvas",
    "Typescript"
  ],
  "author": "Jesus Silva Mendez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesussilva/bomberman/issues"
  },
  "homepage": "https://github.com/jesussilva/bomberman#readme",
  "dependencies": {
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}